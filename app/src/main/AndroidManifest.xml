        <activity android:name=".AlertDialogActivity" />
        <activity android:name=".ResourceActivity" />
        <activity android:name=".UiActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".ListViewPracActivity" />
        <activity android:name=".ListViewSimActivity" />
