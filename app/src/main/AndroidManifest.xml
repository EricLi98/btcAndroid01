        <activity android:name=".TableLayoutActivity" />
        <activity android:name=".RelativelayoutActivity" />
        <activity android:name=".SqliteActivity" />
        <activity android:name=".CalculatorActivity">

        </activity>
        <activity android:name=".LifecycleActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="com.inspur"
                    android:scheme="schemodemo" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentActivity" />
        <activity android:name=".IntentActivity1" />
        <activity android:name=".SharedPreferenceActivity" />

        <receiver
